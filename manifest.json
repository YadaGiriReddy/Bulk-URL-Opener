{
  "name": "Bulk URL Opener",
  "version": "2.0",
  "description": "Opens multiple URLs in one click",
  "icons": {
    "128": "images/Logo.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": {
      "128": "images/Logo.png"
    }
  },
  "manifest_version": 3
}